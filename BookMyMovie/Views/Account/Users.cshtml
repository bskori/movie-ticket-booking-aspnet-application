@model IEnumerable<ApplicationUser>

@{ 
    ViewData["Title"] = "List of all users";
}

<div class="row">
    <div class="col-md-6 offset-3">
        <p>
            <h4>List of all users</h4>
        </p>
        <table class="table">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(n => n.Fullname)</th>
                    <th>@Html.DisplayNameFor(n => n.UserName)</th>
                    <th>@Html.DisplayNameFor(n => n.Email)</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
            {
                    <tr>
                        <td>@item.Fullname</td>
                        <td>@item.UserName</td>
                        <td>@item.Email</td>
                    </tr>
            }
            </tbody>
        </table> 
    </div>
</div>